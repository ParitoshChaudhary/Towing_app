<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.towing_app"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/main_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.towing_app.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.Standardequipment"
            android:label="@string/title_activity_standardequipment"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.example.towing_app.Myimages"
            android:label="@string/title_activity_myimages"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.Towing"
            android:label="@string/title_activity_towing"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.towing_app.General_instruction"
            android:label="@string/title_activity_general_instruction"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.Search"
            android:label="@string/title_activity_search"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.AllManufacture"
            android:label="@string/title_activity_all_manufacture"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.AllModels"
            android:label="@string/title_activity_all_models"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.Year"
            android:label="@string/title_activity_year"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.SearchCar"
            android:label="@string/title_activity_search_car"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.TowInfo"
            android:label="@string/title_activity_tow_info"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.GenJumpStart"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.MotorCycleJumpStart"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.UnderVehicle"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.SpecialOil"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.Disclaimer"
            android:label="@string/title_activity_disclaimer"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.TextTowinfo"
            android:label="@string/title_activity_text_towinfo"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.TowImages"
            android:label="@string/title_activity_tow_images"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.ServiceInfo"
            android:label="@string/title_activity_service_info"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.CarInfo"
            android:label="@string/title_activity_car_info"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.TowLimits"
            android:label="@string/title_activity_tow_limits"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.towing_app.Details_table"
            android:label="@string/title_activity_details_table"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.towing_app.JSPrecaution"
            android:label="@string/title_activity_jsprecaution"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>